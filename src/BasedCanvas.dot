digraph lib {
   // rankdir=LR
   subgraph cluster_IBasedCanvas {
      label="IBasedCanvas"
      subgraph cluster_state {
         label="state"
         ctx
         ctxWidth
         ctxHeight

         canvas
         canvasWidth
         canvasHeight

         container
         containerWidth
         containerHeight
      }

   }

   subgraph cluster_BasedCanvasImpl {
      label="BasedCanvasImpl"
      subgraph cluster_private {
         label="private"
         "#ctx"
         "#ctxWidth"
         "#ctxHeight"
         "#canvasWidth"
         "#canvasHeight"
         "#containerWidth"
         "#containerHeight"
      }
      "#ctx" -> ctx
      "#ctxWidth" -> ctxWidth
      "#ctxHeight" -> ctxHeight
      "#canvasWidth" -> canvasWidth
      "#canvasHeight" -> canvasHeight
      "#containerWidth" -> containerWidth
      "#containerHeight" -> containerHeight
   }
}
